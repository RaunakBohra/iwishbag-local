{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowSESPuts",
      "Effect": "Allow",
      "Principal": {
        "Service": "ses.amazonaws.com"
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::iwishbag-emails/*",
      "Condition": {
        "StringEquals": {
          "AWS:SourceAccount": "597088020156",
          "AWS:SourceArn": "arn:aws:ses:us-east-1:597088020156:receipt-rule-set/*"
        }
      }
    },
    {
      "Sid": "AllowIAMUserAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::597088020156:user/iwishbag-ses"
      },
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::iwishbag-emails",
        "arn:aws:s3:::iwishbag-emails/*"
      ]
    }
  ]
}